program test;
var m: integer;
    n: integer;
    x: integer;
    y: integer;

function min(x: integer; y: integer): integer;
Begin
    if x < y then min := x else min:= y;
End;

function max(x: integer; y: integer): integer;
Begin
    if x < y then max := y else max:= x;
End;

Begin
    x := READI;
    y := READI;
    m := max(x, y);
    n := min(x, y);
    Call WriteI(m - n);
end.