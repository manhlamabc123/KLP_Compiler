PROGRAM Sum;

Var a: Integer; b: Integer;

Function Sum (a: integer; b: integer) : integer;
    BEGIN 
        Sum := a + b;
    END;
BEGIN
    a := READI;
    b := READI;
    CALL WRITELN;
    CALL WRITEI (Sum(a,b));
END.

